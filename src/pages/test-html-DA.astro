<!-- Home Page --><!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home</title>
    <link rel="stylesheet" href="styles.css" />
  </head>
  <body>
    <nav>
      <ul>
        <li><a href="#">Home</a></li>
        <li><a href="#">About</a></li>
        <li><a href="#">Projects</a></li>
      </ul>
    </nav>

    <header class="header-container">
      <div class="header-textbox">
        <h1 class="home-title">Home Page</h1>
      </div>
    </header>

    <main class="content-container">
      <section class="home-background">
        <div class="home-overlay">
          <h2>Welcome</h2>
          <p>Introductory content goes here.</p>
        </div>
      </section>

      <section class="home-scroll">
        <img src="image1.jpg" alt="Image 1" />
        <img src="image2.jpg" alt="Image 2" />
        <img src="image3.jpg" alt="Image 3" />
        <img src="image4.jpg" alt="Image 4" />
        <img src="image5.jpg" alt="Image 5" />
      </section>
    </main>

    <footer class="nav-footer-container two-column-container">
      <div class="footer-column">
        <h2>Newsletter</h2>
        <div class="newsletter">
          <input type="email" placeholder="Enter your email" />
          <button class="button-54">Subscribe</button>
        </div>
      </div>
      <div class="footer-column">
        <h2>Social</h2>
        <div class="social-links">
          <a href="#" class="link-icon">Link 1</a>
          <a href="#" class="link-icon">Link 2</a>
          <a href="#" class="link-icon">Link 3</a>
        </div>
      </div>
    </footer>
  </body>
</html>

